<div class="container">
  <div class="row">
    <%= form_for(:session, url: sessions_path, :html =>{:class => "col s12"}) do |f| %>
      <div class="row">
        <div class="input-field col s6">
          <%= f.text_field :email %>
          <%= f.label :email %>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s6">
          <%= f.password_field :password %>
          <%= f.label :password %>
        </div>
      </div>
      <%= f.submit(:class => "btn", :value => "Login", :id => "location_sync")%>
    <% end %>
  </div>
</div>

