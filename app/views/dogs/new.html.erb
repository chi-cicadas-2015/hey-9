<h1>Dogs#new</h1>
<%= form_for(:dog, url: user_dogs_path(@user.id)) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :bio %>
  <%= f.text_field :bio %>
  <%= f.label :profile_img %>
  <%= f.text_field :profile_img %>
  <%= f.hidden_field :owner_id, :value => @user.id %>
  <%= f.submit %>
<% end %>
