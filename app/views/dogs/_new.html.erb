<div class="container">
  <div class="row">
    <%= form_for(:dog, url: user_dogs_path(@user.id), :html => {:class => "col s12"}) do |f| %>
      <div class="row">
        <div class="input-field col s6">
          <%= f.label :name %>
          <%= f.text_field :name %>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s6">
          <%= f.label :bio %>
          <%= f.text_field :bio %>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s6">
          <%= f.label :profile_img %>
          <%= f.text_field :profile_img %>
        </div>
      </div>
      <%= f.hidden_field :owner_id, :value => current_user.id %>
      <%= f.submit %>
    <% end %>
  </div>
</div>
